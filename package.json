{
  "name": "quizz",
  "version": "1.0.0",
  "description": "Install nest and vue into workspaces, pnpm is required",
  "author": "bidule",
  "type": "module",
  "scripts": {
    "build": "pnpm -r --parallel build",
    "build:api": "pnpm -r --filter api run build",
    "build:ui": "pnpm -r --filter ui run build",
    "dev": "pnpm -r run dev",
    "dev:api": "pnpm -r --filter api run dev",
    "dev:ui": "pnpm -r --filter ui run dev",
    "lint": "pnpm -r --parallel lint",
    "lint:api": "pnpm -r --filter api run lint",
    "lint:ui": "pnpm -r --filter ui run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}